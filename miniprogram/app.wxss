
.continer{
  padding: 0 26rpx;
  box-sizing: border-box;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

@keyframes spark{
  0%, 50%, 100%{opacity: 1;}
  25%, 75%{opacity: 0;}
}